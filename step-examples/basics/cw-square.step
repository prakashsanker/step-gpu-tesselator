ISO-10303-21;
HEADER;
  FILE_DESCRIPTION(('Unit square face example - CW'),'2;1');
  FILE_NAME('square_cw.stp','20250101',(''),(''),'','ChatGPT','');
  FILE_SCHEMA(('CONFIG_CONTROL_DESIGN'));
ENDSEC;
DATA;

  /* ==== Geometric context: 3D, plane in XY with Z up ==== */

  #1 = CARTESIAN_POINT('', (0.0, 0.0, 0.0));              /* origin */
  #2 = DIRECTION('', (0.0, 0.0, 1.0));                    /* Z axis */
  #3 = DIRECTION('', (1.0, 0.0, 0.0));                    /* X axis */
  #4 = AXIS2_PLACEMENT_3D('', #1, #2, #3);                /* placement for plane */
  #5 = PLANE('', #4);                                     /* supporting plane for the face */

  /* ==== Corner points of the square (CW) ==== */
  /* P0 = (0,0,0), P1 = (0,1,0), P2 = (1,1,0), P3 = (1,0,0) */

  #10 = CARTESIAN_POINT('', (0.0, 0.0, 0.0));             /* P0 */
  #11 = CARTESIAN_POINT('', (0.0, 1.0, 0.0));             /* P1 */
  #12 = CARTESIAN_POINT('', (1.0, 1.0, 0.0));             /* P2 */
  #13 = CARTESIAN_POINT('', (1.0, 0.0, 0.0));             /* P3 */

  #20 = VERTEX_POINT('', #10);
  #21 = VERTEX_POINT('', #11);
  #22 = VERTEX_POINT('', #12);
  #23 = VERTEX_POINT('', #13);

  /* ==== Dummy curve directions for edges (we ignore these in your parser) ==== */

  #100 = VECTOR('', #3, 1.0);
  #101 = VECTOR('', #3, 1.0);
  #102 = VECTOR('', #3, 1.0);
  #103 = VECTOR('', #3, 1.0);

  #40 = LINE('', #10, #100);
  #41 = LINE('', #11, #101);
  #42 = LINE('', #12, #102);
  #43 = LINE('', #13, #103);

  /* ==== Edge curves around the square, in CW order ==== */

  #30 = EDGE_CURVE('', #20, #21, #40, .T.);  /* P0 -> P1 */
  #31 = EDGE_CURVE('', #21, #22, #41, .T.);  /* P1 -> P2 */
  #32 = EDGE_CURVE('', #22, #23, #42, .T.);  /* P2 -> P3 */
  #33 = EDGE_CURVE('', #23, #20, #43, .T.);  /* P3 -> P0 */

  #50 = ORIENTED_EDGE('', *, *, #30, .T.);
  #51 = ORIENTED_EDGE('', *, *, #31, .T.);
  #52 = ORIENTED_EDGE('', *, *, #32, .T.);
  #53 = ORIENTED_EDGE('', *, *, #33, .T.);

  #60 = EDGE_LOOP('', (#50, #51, #52, #53));

  #70 = FACE_OUTER_BOUND('', #60, .T.);

  #80 = ADVANCED_FACE('', (#70), #5, .T.);

ENDSEC;
END-ISO-10303-21;