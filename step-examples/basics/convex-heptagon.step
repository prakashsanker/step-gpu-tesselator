ISO-10303-21;
HEADER;
  FILE_DESCRIPTION(('Small convex heptagon face example'),'2;1');
  FILE_NAME('convex_heptagon_small.stp','20250101',(''),(''),'','ChatGPT','');
  FILE_SCHEMA(('CONFIG_CONTROL_DESIGN'));
ENDSEC;
DATA;

  /* ==== Geometric context: 3D, plane in XY with Z up ==== */

  #1 = CARTESIAN_POINT('', (0.0, 0.0, 0.0));              
  #2 = DIRECTION('', (0.0, 0.0, 1.0));                    
  #3 = DIRECTION('', (1.0, 0.0, 0.0));                    
  #4 = AXIS2_PLACEMENT_3D('', #1, #2, #3);
  #5 = PLANE('', #4);

  /* ==== Small heptagon vertices (counter-clockwise) ==== */

  #10 = CARTESIAN_POINT('', ( 2.0,  0.0, 0.0));       /* V0 */
  #11 = CARTESIAN_POINT('', ( 1.3,  1.5, 0.0));       /* V1 */
  #12 = CARTESIAN_POINT('', ( 0.0,  2.0, 0.0));       /* V2 */
  #13 = CARTESIAN_POINT('', (-1.3,  1.5, 0.0));       /* V3 */
  #14 = CARTESIAN_POINT('', (-2.0,  0.2, 0.0));       /* V4 */
  #15 = CARTESIAN_POINT('', (-1.6, -1.3, 0.0));       /* V5 */
  #16 = CARTESIAN_POINT('', ( 0.4, -1.8, 0.0));       /* V6 */

  #20 = VERTEX_POINT('', #10);
  #21 = VERTEX_POINT('', #11);
  #22 = VERTEX_POINT('', #12);
  #23 = VERTEX_POINT('', #13);
  #24 = VERTEX_POINT('', #14);
  #25 = VERTEX_POINT('', #15);
  #26 = VERTEX_POINT('', #16);

  /* ==== Line directions for each edge ==== */

  #30 = DIRECTION('', ( -0.54,  0.84, 0.0));    /* V0→V1 */
  #31 = DIRECTION('', ( -0.65,  0.76, 0.0));    /* V1→V2 */
  #32 = DIRECTION('', ( -0.71, -0.71, 0.0));    /* V2→V3 */
  #33 = DIRECTION('', ( -0.63, -0.78, 0.0));    /* V3→V4 */
  #34 = DIRECTION('', (  0.36, -0.93, 0.0));    /* V4→V5 */
  #35 = DIRECTION('', (  0.88, -0.47, 0.0));    /* V5→V6 */
  #36 = DIRECTION('', (  0.90,  0.44, 0.0));    /* V6→V0 */

  #40 = LINE('', #10, #30);
  #41 = LINE('', #11, #31);
  #42 = LINE('', #12, #32);
  #43 = LINE('', #13, #33);
  #44 = LINE('', #14, #34);
  #45 = LINE('', #15, #35);
  #46 = LINE('', #16, #36);

  #50 = EDGE_CURVE('', #20, #21, #40, .T.);
  #51 = EDGE_CURVE('', #21, #22, #41, .T.);
  #52 = EDGE_CURVE('', #22, #23, #42, .T.);
  #53 = EDGE_CURVE('', #23, #24, #43, .T.);
  #54 = EDGE_CURVE('', #24, #25, #44, .T.);
  #55 = EDGE_CURVE('', #25, #26, #45, .T.);
  #56 = EDGE_CURVE('', #26, #20, #46, .T.);

  /* ==== Oriented edges in CCW loop ==== */

  #60 = ORIENTED_EDGE('', *, *, #50, .T.);
  #61 = ORIENTED_EDGE('', *, *, #51, .T.);
  #62 = ORIENTED_EDGE('', *, *, #52, .T.);
  #63 = ORIENTED_EDGE('', *, *, #53, .T.);
  #64 = ORIENTED_EDGE('', *, *, #54, .T.);
  #65 = ORIENTED_EDGE('', *, *, #55, .T.);
  #66 = ORIENTED_EDGE('', *, *, #56, .T.);

  #70 = EDGE_LOOP('', (#60, #61, #62, #63, #64, #65, #66));
  #80 = FACE_OUTER_BOUND('', #70, .T.);
  #90 = ADVANCED_FACE('', (#80), #5, .T.);

  #100 = CLOSED_SHELL('', (#90));
  #110 = MANIFOLD_SOLID_BREP('CONVEX_HEPTAGON_SMALL', #100);

ENDSEC;
END-ISO-10303-21;
